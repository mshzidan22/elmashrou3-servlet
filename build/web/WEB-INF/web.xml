<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ansServ</servlet-name>
        <servlet-class>ansServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>askServ</servlet-name>
        <servlet-class>askServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>questionAnsServ</servlet-name>
        <servlet-class>questionAnsServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createAccountServ</servlet-name>
        <servlet-class>createAccountServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>takeAnsServ</servlet-name>
        <servlet-class>takeAnsServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewAnswers</servlet-name>
        <servlet-class>viewAnswers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ansServ</servlet-name>
        <url-pattern>/ansServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>askServ</servlet-name>
        <url-pattern>/askServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>questionAnsServ</servlet-name>
        <url-pattern>/questionAnsServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createAccountServ</servlet-name>
        <url-pattern>/createAccountServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>takeAnsServ</servlet-name>
        <url-pattern>/takeAnsServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewAnswers</servlet-name>
        <url-pattern>/viewAnswers</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
